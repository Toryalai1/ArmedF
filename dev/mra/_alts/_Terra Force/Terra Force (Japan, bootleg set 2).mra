      <part name="f-8.3m" crc="d1014280" map="10"/>
    </interleave>
    <part repeat="0x30000"> ff</part>

    <!-- foreground - starts at 0x80000 -->
    <part name="f-6.3c" crc="25d23dfd"/>
    <part name="f-7.3e" crc="b9b0fe27"/>
