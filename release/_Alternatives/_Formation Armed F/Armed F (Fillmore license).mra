<misterromdescription>
    <name>Armed F (Fillmore license)</name>
    <mameversion>0240</mameversion>
    <setname>armedff</setname>
    <year>1987</year>
    <manufacturer>Nichibutsu</manufacturer>
    <category>Shoot 'em up</category>
    <rbf>terraforce</rbf>

    <switches default="60,30">
        <!-- DSW1 -->
        <dip name="Lives" bits="0,1" ids="3,4,5,6"/>
        <dip name="Bonus Life" bits="2,3" ids="20k/60k(E),40k/60k(E),20k/80k(E),40k/80k(E)"/>
        <dip name="Demo Sounds" bits="4" ids="On,Off"/>
        <dip name="Cabinet" bits="5" ids="Cocktail,Upright"/>
        <dip name="Difficulty" bits="6,7" ids="Easy,Normal,Hard,Hardest"/>
        <!-- DSW2 -->
        <dip name="Coin A" bits="8,9" ids="1/1,1/2,2/1,Free Play"/>
        <dip name="Coin B" bits="10,11" ids="1/1,1/2,2/1,2/3"/>
        <dip name="Allow Continue" bits="12,13" ids="No,3x,5x,Yes"/>
        <dip name="Flip Screen" bits="14" ids="Off,On"/>
    </switches>

    <buttons names="Beam,Formation,Start (1p),Start (2p),Coin,Pause" default="A,B,X,Y,R,Start"/>

    <rom index="0" zip="armedff.zip|armedf.zip" md5="None">

        <!-- maincpu - starts at 0x0 -->
        <interleave output="16">
            <part name="af_06.rom" crc="c5326603" map="01"/>
            <part name="af_01.rom" crc="458e9542" map="10"/>
        </interleave>
        <interleave output="16">
            <part name="af_07.rom" crc="cc8517f5" map="01"/>
            <part name="af_02.rom" crc="214ef220" map="10"/>
        </interleave>
        <interleave output="16">
            <part name="af_08.rom" crc="d1d43600" map="01"/>
            <part name="af_03.rom" crc="bbe1fe2d" map="10"/>
        </interleave>

        <!-- foreground - starts at 0x60000 -->
        <part name="af_04.rom" crc="44d3af4f"/>
        <part name="af_05.rom" crc="92076cab"/>

        <!-- background - starts at 0x80000 -->
        <part name="af_14.rom" crc="8c5dc5a7"/>
        <part name="af_13.rom" crc="136a58a3"/>
        <part repeat="0x20000"> FF</part>

        <!-- sprite - starts at 0xC0000 -->
        <interleave output="16">
            <part name="af_11.rom" crc="b46c473c" map="01"/>
            <part name="af_12.rom" crc="23cb6bfe" map="10"/>
        </interleave>

        <!-- text - starts at 0x100000 -->
        <part name="af_09.rom" crc="7025e92d"/>
        <part repeat="0x8000"> FF</part>

        <!-- audiocpu - starts at 0x110000 -->
        <part name="af_10.rom" crc="c5eacb87"/>
        <part repeat="0x8000"> FF</part>

        <!-- nb1414m4 - starts at 0x128000 -->
        <part repeat="0x4000"> FF</part>

        <!-- proms - starts at 0x12C000 -->
        <part repeat="0x4000"> FF</part>

        <!-- Total 0x130000 bytes - 1216 kBytes -->
    </rom>

    <rom index="1">
        <part>01</part>
    </rom>
</misterromdescription>
